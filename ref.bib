@article{friedmann2021growingtreesacquisition,
  title = {Growing Trees: {{The}} Acquisition of the Left Periphery},
  shorttitle = {Growing Trees},
  author = {Friedmann, Naama and Belletti, Adriana and Rizzi, Luigi},
  date = {2021-11-01},
  journaltitle = {Glossa: a journal of general linguistics},
  volume = {6},
  number = {1},
