@article{gast2006rethinkingrelationshipselfintensifiers,
  title = {Rethinking the Relationship between {{SELF-intensifiers}} and Reflexives},
  author = {Gast, Volker and Siemund, Peter},
  date = {2006-01-20},
  journaltitle = {Linguistics},
  volume = {44},
  number = {2},
  issn = {0024-3949, 1613-396X},
  doi = {10.1515/LING.2006.013},
  url = {https://www.degruyter.com/document/doi/10.1515/LING.2006.013/html},
  urldate = {2024-01-31},
  abstract = {Recent studies into the syntax and semantics of intensifying self-forms (e.g. [John HIMSELF] came) have shown that a distinction needs to be drawn between two uses of such expressions: a juxtaposed or adnominal use (cf. above), and a non-juxtaposed use (e.g. John [came HIMSELF]). This differentiation allows us to reconsider a number of issues relating to the synchronic and diachronic relationship between SELFintensifiers and reflexive anaphors. Assessing relevant cross-linguistic data against the background of the aforementioned distinction reveals a surprising fact: patterns of ‘formal relatedness’ suggest a particularly strong empirical as well as conceptual tie-up between reflexives and SELF-intensifiers in their non-juxtaposed rather than adnominal use. This is remarkable because it has generally been assumed that it is always the adnominal SELF-intensifier which gives rise to the development of reflexive markers. In the light of our cross-linguistic findings we explore the synchronic and diachronic relationship between reflexives and SELF-intensifiers in their non-juxtaposed use. We argue that the picture of a (unidirectional) development from adnominal SELF-intensifiers to reflexives needs to be modified insofar as reflexive markers often develop from non-juxtaposed, rather than adnominal, intensifiers. Moreover, reflexive markers often form part of a strategy of SELFintensification, which entails that the reflexives are older than the resulting intensifiers.},
  langid = {english},
  file = {/mnt/chromeos/GoogleDrive/MyDrive/Reading/Gast, Siemund 2004. Rethinking the Relationship Between SELF-Intensifiers.pdf}
}

@article{geist2010baresingularnps,
