@incollection{jalava2017grammaticalizationmodalityevidentiality,
  title = {Grammaticalization of Modality and Evidentiality in {{Tundra Nenets}}},
  booktitle = {The {{Grammaticalization}} of {{Tense}}, {{Aspect}}, {{Modality}} and {{Evidentiality}}},
  author = {Jalava, Lotta},
